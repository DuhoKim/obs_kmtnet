#<?cfg paf policy ?>

needCalibRegistry: true

camera:     "../decam/decam_geom.paf"
defects:    "../decam/defects"
defaultLevel: "Ccd"

exposures: {
    raw: {
        template:    "%(proposal)/%(object)s/%(date)s/dec%(visit)06d.fits.fz[%(side)s%(ccd)d]"
        python:      "lsst.afw.image.DecoratedImageF"
        persistable: "DecoratedImageF"
        storage:     "FitsStorage"
        level:       "Ccd"
        tables:      "raw"
        columns:     "visit"
        columns:     "side"
        columns:     "ccd"
    }
    postISRCCD: {
        template:    "%(proposal)/reduced/%(object)s/%(date)s/postISRCCD-%(visit)06d_%(side)s%(ccd)02d.fits"
        python:      "lsst.afw.image.ExposureF"
        persistable: "ExposureF"
        storage:     "FitsStorage"
        level:       "Ccd"
        tables:      "raw"
        columns:     "visit"
        columns:     "side"
        columns:     "ccd"
    }
    calexp: {
        template:    "%(proposal)/reduced/%(object)s/%(date)s/calexp-%(visit)06d_%(side)s%(ccd)02d.fits"
        python:      "lsst.afw.image.ExposureF"
        persistable: "ExposureF"
        storage:     "FitsStorage"
        level:       "Ccd"
        tables:      "raw"
        columns:     "visit"
        columns:     "side"
        columns:     "ccd"
    }

    coaddTempExp: {
        template:    "%(proposal)/temp/%(tract)d/COADDTEMP--%(patch)s.fits"
        python:      "lsst.afw.image.ExposureF"
        persistable: "ExposureF"
        storage:     "FitsStorage"
        tables:      "raw"
        columns:     "visit"
        columns:     "side"
        columns:     "ccd"
    }

}

calibrations: {
    bias: {
        template:    "BIAS/%(calibDate)s/BIAS-%(calibVersion)d-%(side)s%(ccd)02d.fits"
        python:      "lsst.afw.image.ExposureF"
        persistable: "ExposureF"
        storage:     "FitsStorage"
        level:       "Ccd"
        tables:      "bias"
        columns:     "date"
        reference:   "raw"
        refCols:     "visit"
        refCols:     "side"
        refCols:     "ccd"
        filter:      false
        validRange:  true
        validStartName: "validStart"
        validEndName: "validEnd"
        obsTimeName: "datetime"
    }
    dark: {
        template:    "DARK/%(calibDate)s/DARK-%(calibVersion)d-%(side)s%(ccd)02d.fits"
        python:      "lsst.afw.image.ExposureF"
        persistable: "ExposureF"
        storage:     "FitsStorage"
        level:       "Ccd"
        tables:      "dark"
        columns:     "datetime"
        reference:   "raw"
        refCols:     "visit"
        refCols:     "side"
        refCols:     "ccd"
        filter:      false
        validRange:  true
        validStartName: "validStart"
        validEndName: "validEnd"
        obsTimeName: "datetime"
    }
    flat: {
        template:    "FLAT/%(calibDate)s/FLAT-%(calibVersion)d-%(side)s%(ccd)02d.fits"
        python:      "lsst.afw.image.ExposureF"
        persistable: "ExposureF"
        storage:     "FitsStorage"
        level:       "Ccd"
        tables:      "flat"
        columns:     "datetime"
        reference:   "raw"
        refCols:     "visit"
        refCols:     "side"
        refCols:     "ccd"
        filter:      true
        validRange:  true
        validStartName: "validStart"
        validEndName: "validEnd"
        obsTimeName: "datetime"
    }
}



datasets: {
    psf: {
        template:      "%(proposal)/output/%(object)s/%(date)s/PSF-%(visit)06d_%(side)s%(ccd)02d.boost"
        python:        "lsst.afw.detection.Psf"
        persistable:   "Psf"
        storage:       "BoostStorage"
        tables:        "raw"
    }
    apCorr: {
        template:      "%(proposal)/output/%(object)s/%(date)s/APC-%(visit)06d_%(side)s%(ccd)02d.pickle"
        python:        "lsst.meas.algorithms.ApertureCorrection"
        persistable:   "ApertureCorrection"
        storage:       "PickleStorage"
        tables:        "raw"
    }
    src: {
        template:      "%(proposal)/output/%(object)s/%(date)s/SRC-%(visit)06d_%(side)s%(ccd)02d.fits"
        python:        "lsst.afw.table.SourceCatalog"
        persistable:   "SourceCatalog"
        storage:       "FitsCatalogStorage"
        tables:        "raw"
    }
    icMatch: {
        template:      "%(proposal)/output/%(object)s/%(date)s/ICMATCH-%(visit)06d_%(side)s%(ccd)02d.fits"
        python:        "lsst.afw.table.BaseCatalog"
        persistable:   "BaseCatalog"
        storage:       "FitsCatalogStorage"
        tables:        "raw"
    }
    icSrc: {
        template:      "%(proposal)/output/%(object)s/%(date)s/ICSRC-%(visit)06d_%(side)s%(ccd)02d.fits"
        python:        "lsst.afw.table.SourceCatalog"
        persistable:   "SourceCatalog"
        storage:       "FitsCatalogStorage"
        tables:        "raw"
    }
    log: {
        template:      "%(proposal)/logs/%(object)s/%(date)s/run.log"
        python:        "str"
        persistable:   "None"
        storage:       "TextStorage"
    }
    logDir: {
        template:      "%(proposal)/logs/%(object)s/%(date)s/"
        python:        "str"
        persistable:   "None"
        storage:       "None"
    }

    ccdExposureId: {
        template:     "ignored"
        # Provide something to import, although we actually return a long
        python:       "lsst.daf.base.PropertySet"
        persistable:  "ignored"
        storage:      "ignored"
        tables:       "raw"
    }
    ccdExposureId_bits: {
        template:     "ignored"
        # Provide something to import, although we actually return an int
        python:       "lsst.daf.base.PropertySet"
        persistable:  "ignored"
        storage:      "ignored"
        tables:       "raw"
    }

    stackExposureId: {
        template:     "ignored"
        # Provide something to import, although we actually return a long
        python:       "lsst.daf.base.PropertySet"
        persistable:  "ignored"
        storage:      "ignored"
        tables:       "raw"
    }
    stackExposureId_bits: {
        template:     "ignored"
        # Provide something to import, although we actually return an int
        python:       "lsst.daf.base.PropertySet"
        persistable:  "ignored"
        storage:      "ignored"
        tables:       "raw"
    }

    deepCoadd: {
        template:      "%(proposal)/deepCoadd/%(tract)d/%(patch)s.fits"
        python:        "lsst.afw.image.ExposureF"
        persistable:   "ExposureF"
        storage:       "FitsStorage"
        tables:        "raw"
    }
    deepCoadd_depth: {
        template:      "%(proposal)/deepCoadd/%(tract)d/%(patch)s.depth.fits"
        python:        "lsst.afw.image.ImageF"
        persistable:   "ImageF"
        storage:       "FitsStorage"
        tables:        "raw"
    }
    deepCoadd_psf: {
        template:      "%(proposal)/deepCoadd/%(tract)d/%(patch)s.boost"
        python:        "lsst.afw.detection.Psf"
        persistable:   "Psf"
        storage:       "BoostStorage"
        tables:        "raw"
    }
    deepCoadd_skyMap: {
        template:      "%(proposal)/deepCoadd/skyMap.pickle"
        python:        "lsst.skymap.BaseSkyMap"
        persistable:   "ignored"
        storage:       "PickleStorage"
        tables:        "raw"
    }

}
